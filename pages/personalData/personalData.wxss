/* search */
.ui_searchBox{
    height: 100rpx;
    width: 100%;
    position: relative;
    padding: 20rpx 0;
    box-sizing: border-box;
}
.ui_search{
    display: flex;
    width: 75%;
    margin: 0 auto;
    height: 60rpx;
    line-height: 60rpx;
    border: 1px solid #ddd;
    border-radius: 30rpx;
    background: #fff;
}
.ui_searchIcon{
    padding: 15rpx 30rpx 0 15rpx;
    box-sizing: border-box;
}
.ui_searchInput{
    width: 80%;
    height: 100%;
    padding: 5rpx;
    box-sizing: border-box;
}